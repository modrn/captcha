@charset "utf-8";

/* CSS Document */

@font-face {
    font-family: 'robotoregular';
    src: url('../fonts/roboto-regular.eot');
    src: url('../fonts/roboto-regular.eot?#iefix') format('embedded-opentype'), url('../fonts/roboto-regular.woff') format('woff'), url('../fonts/roboto-regular.ttf') format('truetype'), url('../fonts/roboto-regular.svg#robotoregular') format('svg');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'segoe_scriptregular';
    src: url('../fonts/segoesc_0.eot');
    src: url('../fonts/segoesc_0.eot?#iefix') format('embedded-opentype'), url('../fonts/segoesc_0.woff') format('woff'), url('../fonts/segoesc_0.ttf') format('truetype'), url('../fonts/segoesc_0.svg#segoe_scriptregular') format('svg');
    font-weight: normal;
    font-style: normal;
}
.a-non {
    text-decoration: none;
    padding: 0px;
    margin: 0px;
}
.a-non:hover {
    text-decoration: none;
}
input {
    border-radius: 3px;
}
button.main {
    color: #fff;
    font-weight: bold;
    text-transform: uppercase;
    border: 1px solid #68903a;
    border-radius: 3px;
    background: rgb(156, 201, 106);
    /* Old browsers */
    background: -moz-linear-gradient(top, rgba(156, 201, 106, 1) 0%, rgba(126, 180, 69, 1) 100%);
    /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, rgba(156, 201, 106, 1)), color-stop(100%, rgba(126, 180, 69, 1)));
    /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, rgba(156, 201, 106, 1) 0%, rgba(126, 180, 69, 1) 100%);
    /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, rgba(156, 201, 106, 1) 0%, rgba(126, 180, 69, 1) 100%);
    /* Opera 11.10+ */
    background: -ms-linear-gradient(top, rgba(156, 201, 106, 1) 0%, rgba(126, 180, 69, 1) 100%);
    /* IE10+ */
    background: linear-gradient(to bottom, rgba(156, 201, 106, 1) 0%, rgba(126, 180, 69, 1) 100%);
    /* W3C */
    filter: progid: DXImageTransform.Microsoft.gradient(startColorstr='#9cc96a', endColorstr='#7eb445', GradientType=0);
    /* IE6-9 */
    transition: all 0.5s ease;
    box-shadow: 0px 3px 0px 0px rgba(66, 134, 52, 1);
}
.clearfix {
    clear: both;
}
* {
    color: #ebf5f3;
}
p {
    font-size: 18px;
    font-family: 'robotoregular', Arial, sans-serif;
}
a {
    transition: all 0.5s ease;
}
a:hover {
    text-decoration: none !important;
}
h1 {
    font-size: 50px !important;
    font-family: 'robotoregular', Arial, sans-serif;
    margin-bottom: 20px;
    text-transform: uppercase;
}
.carousel-inner > .item {
    -webkit-transition: 0.6s ease-in-out left !important;
    -moz-transition: 0.6s ease-in-out left !important;
    -o-transition: 0.6s ease-in-out left !important;
    transition: 0.6s ease-in-out left !important;
}
h3 {
    font-size: 28px !important;
}
h4 {
    font-size: 24px !important;
}
ul li {
    list-style: none;
}
.header .nav {
    padding-bottom: 200px;
}
/*------------header-------------*/

.header {
    padding: 30px 0px;
    background: url(../img/header-bg.png) no-repeat;
    background-size: cover;
    text-align: center;
}
.header h3 {
    font-family: 'segoe_scriptregular', Arial, sans-serif;
}
.header .navbar {
    background: none;
    border: none;
    text-align: left;
}
.header .navbar-header {
    padding-bottom: 10px;
}
.navbar-header a img {
    margin-top: -10px;
}
.header .navbar-collapse {
    float: right;
}
.header .navbar-header button {
    background: #666;
}
.header .navbar-collapse ul li a {
    color: #fff;
}
a.dropdown-toggle {
    background: none !important;
}
.dropdown-menu {
    min-width: 80px;
}
.dropdown-menu a {
    color: #666;
}
.dropdown-menu li {
    display: block;
    float: none;
}
.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus {
    background-color: #000 !important;
}
.dropdown-menu li:hover {
    background: #000;
}
li.dropdown {
    /*background:#0d1418 !important;*/
    border-radius: 5px;
}
li.dropdown ul.dropdown-menu li:hover a {
    color: #fff !important;
}
li.dropdown ul.dropdown-menu a {
    font-size: 12px;
    color: #008595;
}
li.dropdown ul {
    width: 86px;
    box-shadow: none;
}
.header li.dropdown ul {
    border: none;
}
li.dropdown > a,
li.dropdown .caret {
    color: #333 !important;
}
.navbar-collapse ul li a {
    padding: 10px;
}
.header .biz-logo {
    float: left;
}
.header .navigation {
    float: right;
}
.header h1 {
    color: #fff;
    margin-top: -120px;
}
.header .see-info h3 {
    color: #fff;
}
.header .see-info h4 {
    text-decoration: underline;
}
.header .see-info h3 img {
    margin-top: 25px;
}
.header .see-info h3 + h3 {
    margin-top: -15px;
}
.header .info-input {
    margin-bottom: 20px;
}
input {
    color: #666;
    height: 40px;
    border: 1px solid #CCC;
    background: -webkit-linear-gradient(#ededed, #ffffff);
    background: -o-linear-gradient(#ededed, #ffffff);
    background: -moz-linear-gradient(#ededed, #ffffff);
    background: linear-gradient(#ededed, #ffffff);
    margin-bottom: 10px
}
input:hover {
    border: 1px solid #39F;
    background: #fff;
}
/*---------content--------*/

.content {
    background: #f8f9fb;
    padding: 60px 0px 40px 0px;
}
.content h2 {
    text-align: center;
    color: #666666;
}
.content * {
    color: #333!important;
}
/*---------templates--------*/

.templates {
    background: #f8f9fb;
    text-align: center;
    padding: 60px 0px 40px 0px;
}
.templates h2,
.templates h3 {
    color: #666666;
}
.templates h3 {
    font-family: 'segoe_scriptregular', Arial, sans-serif;
}
.templates .template-text {
    padding: 0px 219px;
    margin-top: 30px;
    margin-bottom: 40px;
}
.templates .slide {
    margin-left: 30px !important;
}
.templates .template-text p {
    color: #999999;
}
.flexslider {
    background: none !important;
    padding-top: 0px;
}
.template-slider .slide {
    margin-right: -20px;
}
.templates-slider a > img {
    /*    height: 200px;*/
    /*    margin: 0 20px;*/
    padding: 10px;
}
.templates-slider a .mask {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background-color: rgba(41, 128, 185, 0.9);
    color: #fff;
    border-radius: 3px;
}
.templates-slider a > .mask:hover {
    opacity: 1;
}
.flex-direction-nav {
    bottom: -30px;
    position: absolute;
    left: 52%;
}
.inner-slide {
    float: left;
    width: 23%;
    margin-right: 15px;
}
.flexslider .flex-direction-nav a,
.flexslider .flex-direction-nav a {
    display: block !important;
    /* I added this line */
}
.flex-direction-nav .flex-next {
    margin-right: 0px;
    opacity: 1 !important;
}
.flex-direction-nav .flex-prev {
    opacity: 1 !important;
}
.flexslider {
    width: 100%;
    margin: 0 auto;
}
/*--------features--------*/

.features {
    background: #3a3a3a;
    text-align: center;
    padding-top: 70px;
    padding-bottom: 40px;
}
.features h2 {
    color: #ebf5f3;
    text-transform: uppercase;
}
.features h3 {
    color: #ebf5f3;
    font-size: 28px;
    margin-bottom: 40px;
    font-family: 'segoe_scriptregular', Arial, sans-serif;
}
.features img {
    width: 60%;
}
.features .feature-list {
    margin-top: 55px;
}
.features h4 {
    color: #3ec0ff;
    font-family: 'segoe_scriptregular', Arial, sans-serif;
}
.features p {
    font-size: 18px;
}
/*-------special features---------*/

.special-features {
    background: url(../img/sp-features.bg.png) repeat;
    text-align: center;
    border-top: 1px solid #797a7b;
    padding: 50px 0px;
    margin-top: 0px !important;
}
.special-features h3 {
    color: #3ec0ff;
    font-family: 'segoe_scriptregular', Arial, sans-serif;
}
.special-features p {
    line-height: 30px;
    font-size: 18px;
}
.special-features .slick-dots li {} .special-features .slick-dots li button:before {
    font-size: 14px;
    color: #cacaca;
    background: transparent;
}
.special-features .slick-dots li.slick-active button:before {
    color: #ffeb00;
}
.special-features .slick-prev {
    left: 0px!important;
}
.special-features .slick-next {
    right: 0!important;
}
